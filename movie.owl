<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ma "http://www.semanticweb.org/ontologies/ECS735/movie.owl#" >
]>


<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/ECS735/movie.owl#"
     xml:base="http://www.semanticweb.org/ontologies/ECS735/movie.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ma="http://www.semanticweb.org/ontologies/ECS735/movie.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/ECS735/movie.owl">
        <rdfs:comment rdf:datatype="&xsd;string">A movie ontology for Semantic Web module coursework (http://qmplus.qmul.ac.uk/course/view.php?id=2405)</rdfs:comment>
        <owl:versionInfo rdf:datatype="&xsd;string">version 1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#associated_with -->

    <owl:ObjectProperty rdf:about="&ma;associated_with">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Movie"/>
                    <rdf:Description rdf:about="&ma;MovieAssociatedProduct"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Movie"/>
                    <rdf:Description rdf:about="&ma;MovieAssociatedProduct"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#awarded_at -->

    <owl:ObjectProperty rdf:about="&ma;awarded_at">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/FilmFestival"/>
        <rdfs:domain rdf:resource="&ma;MovieAward"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#awarded_to -->

    <owl:ObjectProperty rdf:about="&ma;awarded_to">
        <rdfs:domain rdf:resource="&ma;MovieAward"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Movie"/>
                    <rdf:Description rdf:about="&ma;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#cinematographer_of -->

    <owl:ObjectProperty rdf:about="&ma;cinematographer_of">
        <rdfs:domain rdf:resource="&ma;Cinematographer"/>
        <rdfs:range rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;cinematography_by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#cinematography_by -->

    <owl:ObjectProperty rdf:about="&ma;cinematography_by">
        <rdfs:range rdf:resource="&ma;Cinematographer"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#composed_by -->

    <owl:ObjectProperty rdf:about="&ma;composed_by">
        <rdfs:range rdf:resource="&ma;Composer"/>
        <rdfs:domain rdf:resource="&ma;Soundtrack"/>
        <owl:inverseOf rdf:resource="&ma;composer_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#composer_of -->

    <owl:ObjectProperty rdf:about="&ma;composer_of">
        <rdfs:domain rdf:resource="&ma;Composer"/>
        <rdfs:range rdf:resource="&ma;Soundtrack"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#directed_by -->

    <owl:ObjectProperty rdf:about="&ma;directed_by">
        <rdfs:range rdf:resource="&ma;Director"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#director_of -->

    <owl:ObjectProperty rdf:about="&ma;director_of">
        <rdfs:domain rdf:resource="&ma;Director"/>
        <rdfs:range rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;directed_by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#distributed_by -->

    <owl:ObjectProperty rdf:about="&ma;distributed_by">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;distributer_of"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Distributor"/>
                    <rdf:Description rdf:about="&ma;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#distributer_of -->

    <owl:ObjectProperty rdf:about="&ma;distributer_of">
        <rdfs:range rdf:resource="&ma;Movie"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Distributor"/>
                    <rdf:Description rdf:about="&ma;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#edited_by -->

    <owl:ObjectProperty rdf:about="&ma;edited_by">
        <rdfs:range rdf:resource="&ma;Editor"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;editor_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#editor_of -->

    <owl:ObjectProperty rdf:about="&ma;editor_of">
        <rdfs:domain rdf:resource="&ma;Editor"/>
        <rdfs:range rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#featured_in -->

    <owl:ObjectProperty rdf:about="&ma;featured_in">
        <rdfs:domain rdf:resource="&ma;Actor"/>
        <rdfs:range rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;features_actor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#features_actor -->

    <owl:ObjectProperty rdf:about="&ma;features_actor">
        <rdfs:range rdf:resource="&ma;Actor"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#has_award -->

    <owl:ObjectProperty rdf:about="&ma;has_award">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/FilmFestival"/>
        <rdfs:range rdf:resource="&ma;MovieAward"/>
        <owl:inverseOf rdf:resource="&ma;awarded_at"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#has_filming_location -->

    <owl:ObjectProperty rdf:about="&ma;has_filming_location">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&ma;Place"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#has_genre -->

    <owl:ObjectProperty rdf:about="&ma;has_genre">
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/MovieGenre"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#has_language -->

    <owl:ObjectProperty rdf:about="&ma;has_language">
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Language"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#has_production_country -->

    <owl:ObjectProperty rdf:about="&ma;has_production_country">
        <rdfs:range rdf:resource="&ma;Country"/>
        <rdfs:domain rdf:resource="&ma;Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#produced_by -->

    <owl:ObjectProperty rdf:about="&ma;produced_by">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&ma;Producer"/>
        <owl:inverseOf rdf:resource="&ma;producer_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#producer_of -->

    <owl:ObjectProperty rdf:about="&ma;producer_of">
        <rdfs:range rdf:resource="&ma;Movie"/>
        <rdfs:domain rdf:resource="&ma;Producer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#released_by -->

    <owl:ObjectProperty rdf:about="&ma;released_by">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Person"/>
                    <rdf:Description rdf:about="&ma;Studio"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#released_movie -->

    <owl:ObjectProperty rdf:about="&ma;released_movie">
        <rdfs:range rdf:resource="&ma;Movie"/>
        <owl:inverseOf rdf:resource="&ma;released_by"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Person"/>
                    <rdf:Description rdf:about="&ma;Studio"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#winner_of -->

    <owl:ObjectProperty rdf:about="&ma;winner_of">
        <rdfs:range rdf:resource="&ma;MovieAward"/>
        <owl:inverseOf rdf:resource="&ma;awarded_to"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;Movie"/>
                    <rdf:Description rdf:about="&ma;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#writer_of -->

    <owl:ObjectProperty rdf:about="&ma;writer_of">
        <rdfs:range rdf:resource="&ma;Screenplay"/>
        <rdfs:domain rdf:resource="&ma;Writer"/>
        <owl:inverseOf rdf:resource="&ma;written_by"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#written_by -->

    <owl:ObjectProperty rdf:about="&ma;written_by">
        <rdfs:domain rdf:resource="&ma;Screenplay"/>
        <rdfs:range rdf:resource="&ma;Writer"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#birth_date -->

    <owl:DatatypeProperty rdf:about="&ma;birth_date">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&ma;Person"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#budget -->

    <owl:DatatypeProperty rdf:about="&ma;budget">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#gross -->

    <owl:DatatypeProperty rdf:about="&ma;gross">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#name -->

    <owl:DatatypeProperty rdf:about="&ma;name">
        <rdfs:domain rdf:resource="&ma;MovieAgent"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#plot_summary -->

    <owl:DatatypeProperty rdf:about="&ma;plot_summary">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#rating -->

    <owl:DatatypeProperty rdf:about="&ma;rating">
        <rdfs:comment xml:lang="en">Movie content rating, e.g. MPAA film rating.</rdfs:comment>
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#release_date -->

    <owl:DatatypeProperty rdf:about="&ma;release_date">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#runtime -->

    <owl:DatatypeProperty rdf:about="&ma;runtime">
        <rdfs:comment xml:lang="en">Movie duration in seconds.</rdfs:comment>
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#title -->

    <owl:DatatypeProperty rdf:about="&ma;title">
        <rdfs:domain rdf:resource="&ma;Movie"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/Event -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Event">
        <rdfs:label xml:lang="en">Event</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/Event</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/FilmFestival -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/FilmFestival">
        <rdfs:label xml:lang="en">Film Festival</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/ontology/Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ma;has_award"/>
                <owl:someValuesFrom rdf:resource="&ma;MovieAward"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/FilmFestival</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Film festival / Movie award ceremony.</rdfs:comment>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/Genre -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Genre">
        <rdfs:label xml:lang="en">Genre</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/Genre</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/Language -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Language">
        <rdfs:label xml:lang="en">Language</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/Language</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Language in which the movie was filmed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/MovieGenre -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/MovieGenre">
        <rdfs:label xml:lang="en">Movie Genre</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/ontology/Genre"/>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/MovieGenre</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Various film genres.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Actor -->

    <owl:Class rdf:about="&ma;Actor">
        <rdfs:label xml:lang="en">Actor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Person"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#AdaptedScreenplay -->

    <owl:Class rdf:about="&ma;AdaptedScreenplay">
        <rdfs:label xml:lang="en">Adapted Screenplay</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Screenplay"/>
        <owl:disjointWith rdf:resource="&ma;OriginalScreenplay"/>
        <rdfs:comment xml:lang="en">Movie script adapted from another source (a novel, play, film, etc.)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Cinematographer -->

    <owl:Class rdf:about="&ma;Cinematographer">
        <rdfs:label xml:lang="en">Cinematographer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Person"/>
        <rdfs:comment xml:lang="en">Director of photography.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Composer -->

    <owl:Class rdf:about="&ma;Composer">
        <rdfs:label xml:lang="en">Composer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Person"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Country -->

    <owl:Class rdf:about="&ma;Country">
        <rdfs:label xml:lang="en">Country</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Place"/>
        <rdfs:comment xml:lang="en">Country of production.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#CreativeWork -->

    <owl:Class rdf:about="&ma;CreativeWork">
        <rdfs:label xml:lang="en">Creative Work</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Director -->

    <owl:Class rdf:about="&ma;Director">
        <rdfs:label xml:lang="en">Director</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Person"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Distributor -->

    <owl:Class rdf:about="&ma;Distributor">
        <rdfs:label xml:lang="en">Distributor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Organization"/>
        <rdfs:comment xml:lang="en">Company responsible for marketing of a film.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Editor -->

    <owl:Class rdf:about="&ma;Editor">
        <rdfs:label xml:lang="en">Editor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Person"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Film -->

    <owl:Class rdf:about="&ma;Film">
        <rdfs:label xml:lang="en">Film</rdfs:label>
        <owl:equivalentClass rdf:resource="&ma;Movie"/>
        <rdfs:subClassOf rdf:resource="&ma;CreativeWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#LicensedSoundtrack -->

    <owl:Class rdf:about="&ma;LicensedSoundtrack">
        <rdfs:label xml:lang="en">Licensed Soundtrack</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;Soundtrack"/>
        <rdfs:comment xml:lang="en">Movie soundtrack containing songs not explicitly written and recorded for the movie itself.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#Movie -->

    <owl:Class rdf:about="&ma;Movie">
        <rdfs:label xml:lang="en">Movie</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ma;CreativeWork"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ma;produced_by"/>
                <owl:onClass rdf:resource="&ma;Producer"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ma;directed_by"/>
                <owl:onClass rdf:resource="&ma;Director"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ma;has_genre"/>
                <owl:someValuesFrom rdf:resource="http://dbpedia.org/ontology/MovieGenre"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#MovieAgent -->

    <owl:Class rdf:about="&ma;MovieAgent">
        <rdfs:label xml:lang="en">Movie Agent</rdfs:label>
        <rdfs:comment xml:lang="en">An agent is an entity that acts; Represents people and organizations in film industry.</rdfs:comment>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&ma;Organization"/>
            <rdf:Description rdf:about="&ma;Person"/>
        </owl:disjointUnionOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/ECS735/movie.owl#MovieAssociatedProduct -->

    <owl:Class rdf:about="&ma;MovieAssociatedProduct">
        <rdfs:label xml:lang="en">Movie Associated Product</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ma;CreativeWork"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ma;associated_with"/>
                        <owl:someValuesFrom rdf:resource="&ma;Movie"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ma;associated_with"/>
                        <owl:allValuesFrom rdf:resource="&ma;Movie"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
